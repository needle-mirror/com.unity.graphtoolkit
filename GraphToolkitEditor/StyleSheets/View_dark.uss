.ge-view, .unity-base-dropdown__container-outer {
    --unity-icon-foldout: resource("Builtin Skins/DarkSkin/Images/IN foldout@2x.png");
    --unity-icon-foldout-on: resource("Builtin Skins/DarkSkin/Images/IN foldout on@2x.png");
    --unity-icon-foldout-act: resource("Builtin Skins/DarkSkin/Images/IN foldout act@2x.png");
    --unity-icon-foldout-act-on: resource("Builtin Skins/DarkSkin/Images/IN foldout act on@2x.png");
    --unity-icon-foldout-focus: resource("Builtin Skins/DarkSkin/Images/IN foldout focus@2x.png");
    --unity-icon-foldout-focus-on: resource("Builtin Skins/DarkSkin/Images/IN foldout focus on@2x.png");

    --overlay-background-color: #383838;

    --override-bar: #C4C4C4;

    --button-add-more: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_Add@2x.png");

    /*ErrorMarker.uss*/
    --graph-color-error: #FF534A;
    --graph-color-warning: #FFC107;

    /*Node colors*/
    --graph-color-node-background: #383838;
    --graph-color-node-background-dark: #303030;
    --graph-color-node-border: #666666;
    --graph-color-node-selection-highlight: #44C0FF;
    --graph-color-node-hover-highlight: rgba(68, 192, 255, 0.5); /*#44C0FF with 50% alpha*/
    --graph-color-dividers: #222222;
    --graph-color-selection-fill: rgba(68, 192, 255, 0.11);

    /*State.uss*/
    --graph-color-state-state-machine-header-background: #4B88AC;
    --graph-state-state-machine-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/States/d_StateMachineAsset@256.png");
    --graph-enter-state-state-machine-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/States/StateMachineAsset@256.png");
    --graph-color-enter-state-background: #C8C8C8;
    --graph-color-enter-state-label-color: #090909;

    /*Blackboard.uss*/
    --graph-color-blackboard-background: rgba(0, 0, 0, 0.8);
    --blackboard-background: var(--graph-color-blackboard-background);
    --blackboard-header-background: #2A2A2A;
    --blackboard-section-border-line: #313131;
    --blackboard-section-title-background: black;
    --blackboard-group-background: #1A1A23;
    --blackboard-alt-group-background: #191A1F;
    --blackboard-group-border: #5E73B5;

    /*NodeToolbarIconButton.uss*/
    --graph-color-node-icon-button-hover-background: var(--unity-colors-button-background-focus);

    /*ConstantField.uss*/
    --graph-color-plugged-value-tint: #243333;

    /*Wire.uss*/
    --graph-color-wire: #C4C4C4;
    --graph-color-wire-ghost: #C4C4C4;
    --graph-local-transition-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/AnyStateTransition@256.png");
    --graph-local-transition-icon-selected: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/AnyStateTransition@256.png");
    --graph-self-transition-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_SelfTransition@256.png");
    --graph-self-transition-icon-selected: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_SelfTransition@256.png");
    --graph-on-enter-transition-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/OnEnterTransition@256.png");
    --graph-on-enter-transition-icon-selected: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/OnEnterTransition@256.png");
    --graph-color-local-transition-arrow-fill: #69E39F;
    --graph-color-self-transition-arrow-fill: var(--graph-color-block-background);
    --graph-color-on-enter-transition-arrow-fill: var(--graph-color-enter-state-background);

    /*WireBubble.uss*/
    --graph-color-wire-bubble-background: rgba(39, 39, 39, 0.9);

    /*BlockNode.uss*/
    --graph-color-block-background: #3F3F3F;
    --graph-color-block-border: #242424;
    --graph-color-empty-block-background: #2A2A2A;
    --graph-color-block-option-background: #383838;

    /*GraphView.uss*/
    --graph-color-canvas-background: #191919;
    --graph-color-canvas-grid-base: #282828;
    --graph-color-canvas-grid: #3E3E3E;

    /*ModelInspector.uss*/
    --graph-color-inspector-background: rgba(0, 0, 0, 0.8);

    /*Minimap.uss*/
    --graph-color-minimap-viewport-border: rgba(230, 230, 230, 0.5);
    --graph-color-minimap-node: rgba(230, 230, 230, 0.5);

    /*Dropdown arrow*/
    --dropdown-arrow: white;

    /*Port.uss & Blackboard.uss*/
    --port-type-execution: #C99700;
    --port-type-string: #BC6B0B;
    --port-type-gameobject: #C4C4C4;
    --port-type-boolean: #A1A3FF;
    --port-type-integer: #57AEFF;
    --port-type-float: #80FFE6;
    --port-type-float2: #14D368;
    --port-type-float3: #FFEA07;
    --port-type-float4: #E78DDC;
    --port-type-textures: #FD8678;
    --port-type-animation: #69E39F;
    --port-type-matrix: #4B88AC;
    --port-type-mesh: #80D8FF;
    --port-type-component: #B2FF59;
    --port-type-empty: #8D8D8D;
    --port-type-misc: #C4C4C4;
    --port-type-physics: #B2FF59;
    --port-type-automatic: white;

    --polymorphic-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/Port/d_PolymorphicPort.png");
    --color-type-icon: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/Port/d_Color@64x.png");

    --transition-to-state-tint: #b8b8b8;
    --transition-state-on-enter: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_OnEnterTransition.png");
    --transition-state-local: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_AnyStateTransition.png");
    --transition-state-self: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_SelfTransition.png");

    --transition-debug-true: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_TrueDebugging.png");
    --transition-debug-false: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_FalseDebugging.png");
    --transition-debug-not-evaluated: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/StateMachine/Transitions/d_PendingDebugging.png");

    --warning-icon: resource("icons/d_console.warnicon.png");

    --icon-add: resource("d_CreateAddNew");

    /* The bracket in the condition editor*/
    --bracket-color: #C4C4C4;

    --chevron-down: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_NodeChevronDown@2x.png");
    --chevron-up: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_NodeChevronUp@2x.png");
    --chevron-right: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_NodeChevronRight@2x.png");
    --chevron-left: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_NodeChevronLeft@2x.png");

    --node-expandable-port-line-color: #737373;
}


.ge-icon--graph-object {
    --unity-image: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_GraphAsset.png");
}

/*Node Icons*/

