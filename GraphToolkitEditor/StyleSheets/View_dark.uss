.ge-icon--set-node,
.item-library-custom-item-set-node__icon {
    --unity-image: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_Set.png");
}

.ge-icon--split-node,
.item-library-custom-item-split-node__icon {
    --unity-image: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_Split.png");
}

.ge-icon--combine-node,
.item-library-custom-item-combine-node__icon {
    --unity-image: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_Combine.png");
