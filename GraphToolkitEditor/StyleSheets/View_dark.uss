.ge-icon--logic-node,
.item-library-custom-item-logic-node__icon {
    --unity-image: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_Logic.png");
}

.ge-icon--mixer-node,
.item-library-custom-item-mixer-node__icon {
    --unity-image: resource("Packages/com.unity.graphtoolkit/GraphToolkitEditor/Icons/d_Mixer.png");
